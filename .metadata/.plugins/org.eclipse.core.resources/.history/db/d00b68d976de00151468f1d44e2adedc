package com.bazlur.java.datatype;

public class Example {
	public static void main(String[] args) {
		byte myByte;
		float myFloat;
		
		myByte = 127;
		myFloat = 199.99f;
		
	    Integer a;
	    int b = 9;
	    a = b;
	}

	byte myByte;
	short myShort;
	char myChar;
	int myInt;
	long myLong;
	float myFloat;
	double myDouble;

}
