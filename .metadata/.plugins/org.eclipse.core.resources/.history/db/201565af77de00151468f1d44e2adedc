package com.bazlur.java.datatype;

import java.util.ArrayList;
import java.util.List;

public class Example {
	public static void main(String[] args) {
		byte myByte;
		float myFloat;

		myByte = 127;
		myFloat = 199.99f;

		Integer a;
		int b = 9;
		a = b;

		List list = new ArrayList();

		// or

		int x = 5;

		boolean result = true;
		char capitalC = 'C';
		byte c = 100;
		short s = 10000;
		int i = 100000;

		// এখানে 26 হচ্ছে ডেসিমাল নাম্বার
		int decVal = 26;
		// এখানে 26 সংখ্যাটি হেক্সাডেসিমেল এ দেখানো হয়েছে
		int hexVal = 0x1a;
		// এখানে 26 সংখ্যাটি বাইনারি-তে এ দেখানো হয়েছে
		int binVal = 0b11010;

		double d1 = 123.4;
		// একি ভ্যালু বৈজ্ঞানিক উপায়ে লেখা হয়েছে
		double d2 = 1.234e2;
		float f1 = 123.4f;
		
		char chr = 'A'; // ক্যারেক্টার লিটারেল
		String name = "Bazlur"; // স্ট্রিং লিটারের
	}

	byte myByte;
	short myShort;
	char myChar;
	int myInt;
	long myLong;
	float myFloat;
	double myDouble;

}
